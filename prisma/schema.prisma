generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Hanyu {
  id        String   @id @unique
  hanzi     String
  pinyin    String
  xinbanya  String
  tone      String?
  img       String?
  source    String?  @default("form-hanyu")
  createdAt DateTime @default(now())
  updatedAt DateTime
}


